{
  "name": "cart-cruise",
  "version": "1.0.0",
  "description": "Ecommerce MERN",
  "main": "server.js",
  "type": "module",
  "proxy": "http://localhost:5000",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Luci04/CartCruise.git"
  },
  "author": "Avinash Shukla",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Luci04/CartCruise/issues"
  },
  "homepage": "https://github.com/Luci04/CartCruise#readme",
  "dependencies": {
    "@ricarso/react-image-magnifiers": "^1.9.0",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.8.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "concurrently": "^7.3.0",
    "nodemon": "^2.0.20"
  }
}
